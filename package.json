{
  "name": "@clebert/app-kit",
  "version": "1.0.2",
  "description": "This package contains shared components for my web apps.",
  "bugs": {
    "url": "https://github.com/clebert/app-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/app-kit.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "sideEffects": false,
  "files": [
    "index.html",
    "lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc --build",
    "postbuild": "webpack --mode production",
    "ci": "run-p format:test lint test build",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:build": "webpack --mode development --watch",
    "watch:compile": "tsc --build --watch"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "history": "^4.10.1",
    "node-fetch": "^2.6.0",
    "universal-base64": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.47",
    "@types/classnames": "^2.2.10",
    "@types/cookie": "^0.3.3",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/webpack": "^4.41.10",
    "ironhook": "^0.7.0",
    "jest": "^25.2.7",
    "npm-run-all": "^4.1.5",
    "p-defer": "^3.0.0",
    "preact": "^10.4.0",
    "prettier": "^2.0.4",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "preact": "^10.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
